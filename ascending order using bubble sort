	AREA PROGRAM, CODE, READONLY
	ENTRY
MAIN
	MOV R0, #05
LOOP1
	LDR R5, VALUE1
	ADD R6, R5, #1
	MOV R3, #4
LOOP2
	LDRB R1, [R5]
	LDRB R2, [R6]
	CMP R1,R2
	BLO LOOP3
	MOV R4,R2
	MOV R2,R1
	MOV R1,R4
LOOP3
	STRB R1,[R5]
	STRB R2,[R6]
	ADD R5, R5,#1
	ADD R6, R6,#1
	SUBS R3,R3,#1
	BNE LOOP1
	AREA PROGRAM, DATA, READONLY
VALUE1 DCD &00001000
	END
